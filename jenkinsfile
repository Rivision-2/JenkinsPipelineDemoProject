pipeline{
agent any
stages 
{
stage('Build') 
{
steps{
echo "Building the Code.........."
mvn "mvn clean"
}
}
stage('Test') 
{
steps{
echo "Testing the Code.........."
mvn "mvn test"
}
}
stage('Compile') 
{
steps{
echo "Compiling the Project.........."
mvn "mvn compile"
}
}
stage('Deploy') 
{
steps{
echo "Deploying the Project.........."
}
}
}
}
